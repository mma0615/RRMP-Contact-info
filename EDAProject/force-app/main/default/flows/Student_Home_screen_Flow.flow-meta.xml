            <fieldType>RadioButtons</fieldType>
            <isRequired>true</isRequired>
        </fields>
        <fields>
            <name>TheNDUSecurityOffice</name>
            <fieldText>&lt;p&gt;&lt;span style=&quot;font-size: 14px;&quot;&gt;The NDU Security Office highly recommends that students complete the CAC issuing/reissuing process as soon as possible to ensure base access and a smooth in-processing. To obtain a CAC, make a RAPIDS appointment with the &lt;/span&gt;&lt;b style=&quot;font-size: 14px;&quot;&gt;NDU Security Office in Marshall Hall, Building 62, Room 199A&lt;/b&gt;&lt;span style=&quot;font-size: 14px;&quot;&gt;. You must check in at the Marshall Hall Guard Desk prior to proceeding to the NDU Security Office.&lt;/span&gt;&lt;/p&gt;</fieldText>
            <fieldType>DisplayText</fieldType>
            <visibilityRule>
                <conditionLogic>and</conditionLogic>
                <conditions>
                    <leftValueReference>No4</leftValueReference>
                    <operator>WasSelected</operator>
                    <rightValue>
                        <booleanValue>true</booleanValue>
                    </rightValue>
                </conditions>
            </visibilityRule>
        </fields>
        <fields>
            <name>Have_you_completed_your_NATO_Awareness_Briefing_Certificate</name>
