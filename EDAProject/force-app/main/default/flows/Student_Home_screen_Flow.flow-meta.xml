            <choiceReferences>No5</choiceReferences>
            <dataType>String</dataType>
            <fieldText>Have you completed your NATO Awareness Briefing Certificate?</fieldText>
            <fieldType>RadioButtons</fieldType>
            <isRequired>true</isRequired>
        </fields>
        <fields>
            <name>AllUScivilianstudents</name>
            <fieldText>&lt;p&gt;&lt;span style=&quot;font-size: 14px;&quot;&gt;All US civilian students are required to complete a &lt;/span&gt;&lt;u style=&quot;font-size: 14px; color: rgb(40, 17, 192);&quot;&gt;NATO Awareness Briefing Certificate&lt;/u&gt;&lt;span style=&quot;font-size: 14px;&quot;&gt;. Students will turn in the completed certificate to the NDU Security Office during in-processing.&lt;/span&gt;&lt;/p&gt;</fieldText>
            <fieldType>DisplayText</fieldType>
            <visibilityRule>
                <conditionLogic>and</conditionLogic>
                <conditions>
                    <leftValueReference>No5</leftValueReference>
                    <operator>WasSelected</operator>
