        <fields>
            <name>Have_you_submitted_a_copy_of_immunization_records_to_NDU_s_Health_Fitness_Office</name>
            <choiceReferences>Yes6</choiceReferences>
            <choiceReferences>No6</choiceReferences>
            <dataType>String</dataType>
